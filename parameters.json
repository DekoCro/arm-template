{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "Namespace": {
            "minLength": 6,
            "maxLength": 50,
            "type": "String",
            "metadata": {
                "description": "Use Event Hub Namespace."
            }
        },
        "Account": {
            "minLength": 2,
            "maxLength": 50,
            "type": "String",
            "metadata": {
                "description": "Use your company name."
            }
        },
        "EventHub": {
            "minLength": 2,
            "maxLength": 50,
            "type": "String",
            "metadata": {
                "description": "Use Event Hub instance name."
            }
        },
        "Publisher": {
            "type": "String",
            "metadata": {
                "description": "."
            }
        },
        "SharedAccessSignature": {
            "type": "String",
            "metadata": {
                "description": "SharedAccessSignature sr={namespace}.servicebus.windows.net%2f{account}&sig={token}%3d&se=1593090331&skn={account}"
            }
        },
        "PolicyName": {
            "type": "String",
            "metadata": {
                "description": "Shared Access Signature policy name in Event Hub"
            }
        },
        "ProcessingTier": {
            "defaultValue": "Basic",
            "allowedValues": ["Basic", "Standard"],            
            "type": "String",
            "metadata": {
                "description": "The type of replication to use for the storage account."
            }
        }
        
    }
}